<ion-header>
  <ion-navbar color="primary">
    <ion-title>Congressional Staff</ion-title>
  </ion-navbar>
</ion-header>

<ion-content padding>

    <ion-searchbar (ionInput)="getItems($event)"></ion-searchbar>
    <ion-list>
      <ion-item *ngFor="let item of items">
        {{ item }}
      </ion-item>
    </ion-list>



    <ion-list>
        <ion-item>
          <ion-avatar item-start>
            <img src="img/avatar-cher.png">
          </ion-avatar>
          <h2>John Black</h2>
          <p>Convergence</p>
        </ion-item>
      </ion-list>









</ion-content>
